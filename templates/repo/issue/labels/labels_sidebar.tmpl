<div class="ui labels list">
	<span class="no-select item {{if .ctx.HasSelectedLabel}}hide{{end}}">{{.ctx.i18n.Tr "repo.issues.new.no_label"}}</span>
	{{range .ctx.Labels}}
		{{template "repo/issue/labels/label" dict "root" $ "label" .}}
	{{end}}
	{{range .ctx.OrgLabels}}
		{{template "repo/issue/labels/label" dict "root" $ "label" .}}
	{{end}}
</div>
